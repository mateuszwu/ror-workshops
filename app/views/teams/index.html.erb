<h3>
  Teams
  <small class="text-muted"><%= link_to "New team", new_team_path, class: "btn btn-primary" %></small>
</h3>

<div class="container"> <%= form_with url: search_path, method: :get do |form| %>
  <%= form.label :query, "Search for:" %>
  <%= form.text_field :query %>
  <%= form.submit "Search" %>
<%end %>


</div>

<div id="teams" class="d-flex flex-row flex-wrap bd-highlight mb-3 gap-3">
  <% @teams.each do |team| %>
  <%= render :partial => team %>
  <% end %>
    
</div>

